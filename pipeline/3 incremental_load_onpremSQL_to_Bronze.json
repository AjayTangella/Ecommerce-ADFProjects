							"onInactiveMarkAs": "Succeeded",
							"dependsOn": [
								{
									"activity": "Copy data from onprem to bronze layer",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,

										},
										"FileName": {
											"value": "@concat(item().SchemaName,'-',item().TableName,'.csv')",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Update the watermark table",
							"type": "SqlServerStoredProcedure",

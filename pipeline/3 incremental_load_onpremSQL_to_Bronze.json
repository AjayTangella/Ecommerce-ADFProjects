								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": {
										"value": "select max(ModifiedDate) ModifiedDate from @{item().SchemaName}.@{item().TableName}",

{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Efn-Proj"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://efnblobproj.dfs.core.windows.net"
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://efn-proj-kv.vault.azure.net/"
		},
		"ls_Azure_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "efnproject.database.windows.net"
		},
		"ls_Azure_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Efnproject"
		},
		"ls_Azure_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ls_onprem_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "AJAY"
		},
		"ls_onprem_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Efnproject"
		},
		"ls_onprem_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Efn-Proj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_Azure_SQL_properties_typeProperties_server": {
						"value": "[parameters('ls_Azure_SQL_properties_typeProperties_server')]"
					},
					"ls_Azure_SQL_properties_typeProperties_database": {
						"value": "[parameters('ls_Azure_SQL_properties_typeProperties_database')]"
					},
					"ls_Azure_SQL_properties_typeProperties_userName": {
						"value": "[parameters('ls_Azure_SQL_properties_typeProperties_userName')]"
					},
					"ls_onprem_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_onprem_sql_properties_typeProperties_server')]"
					},
					"ls_onprem_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_onprem_sql_properties_typeProperties_database')]"
					},
					"ls_onprem_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_onprem_sql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}